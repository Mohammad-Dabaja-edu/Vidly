@model System.Collections.Generic.List<Vidly.Models.Movie>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Movies</h2>
@if (Model.Count == 0)
{
    <p>we dont have movies</p>
}
else
{
    <table class="table table-bordered">
        <tr><th>Movies</th></tr>
        @foreach(var movie in Model)
        {
            <tr><td>@movie.Name</td></tr>
        }
    </table>
}
